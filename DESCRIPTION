    'doc_distance.r'
    'doc_states.r'
    'doc_distTab.r'
    'doc_FHFA_divs.r'
    'doc_coord_states.r'
    'doc_medinc_2012.r'
    'doc_Migration.r'
    'doc_ownership.r'
    'doc_mortgage.r'
